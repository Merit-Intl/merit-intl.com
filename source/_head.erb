<meta charset="utf-8" />
<meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
<meta name="author" content="<%= @website.title %>" />

<% if current_article && current_article.title %>
	<title><%= current_article.title %></title>
 <% else %>
	<title><%= @website.title %></title>
<% end %>

<link rel="stylesheet" type="text/css" href="http://merit-intl.com/template/2012-08-15/product.css" />
<link rel="stylesheet" type="text/css" href="http://merit-intl.com/template/2012-08-15/default.css" />
<script src="http://merit-intl.com/template/2012-08-15/menu.js"></script>
<%= stylesheet_link_tag "2013-04-23" %>

<!-- Colorbox -->
<link rel="stylesheet" href="http://merit-intl.com/template/2012-08-15/colorbox.css" />
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script src="http://merit-intl.com/template/2012-08-15/jquery.colorbox-min.js"></script>
<script>
	$(document).ready(function(){
		//Examples of how to assign the ColorBox event to elements
		$(".group3").colorbox({rel:'group3', transition:"none", width:"85%", height:"85%"});
	});
</script>